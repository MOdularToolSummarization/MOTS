<?xml version="1.0" encoding="UTF-8"?>
<PROCESS NAME="REQUIRED">
	<OPTION NAME="CorpusIdToSummarize" TYPE="List<int>">0</OPTION>
	<OPTION NAME="ReadStopWords" TYPE="boolean">false</OPTION>
</PROCESS>
<PROCESS NAME="TF_IDF.TF_IDF">
	<OPTION NAME="LoadModel" TYPE="boolean">false</OPTION>
	<COMPATIBILITY NAME_REF="TF_IDF"></COMPATIBILITY>
</PROCESS>
<PROCESS NAME="LDA.LDA">
	<OPTION NAME="K" TYPE="int">25</OPTION>
	<OPTION NAME="Alpha" TYPE="float">0.5</OPTION>
	<OPTION NAME="Beta" TYPE="float">0.5</OPTION>
	<COMPATIBILITY NAME_REF="LDA"></COMPATIBILITY>
</PROCESS>
<PROCESS NAME="ILP.BiGram_ILP">
	<OPTION NAME="fscFactor" TYPE="int">1</OPTION>
	<OPTION NAME="minSenLength" TYPE="int">5</OPTION>
	<COMPATIBILITY NAME_REF="ILP"></COMPATIBILITY>
</PROCESS>

<SCORING_METHOD NAME="TF_IDF.ScoringSentenceTF_IDF">
	<OPTION NAME="TfIdfThreshold" TYPE="int">10</OPTION>
</SCORING_METHOD>
<SCORING_METHOD NAME="TF_IDF.Centroid">
	<OPTION NAME="NbMaxWordInCentroid" TYPE="int">10</OPTION>
</SCORING_METHOD>
<SCORING_METHOD NAME="LDA.ScoringLDA">
	<OPTION NAME="SimilarityMethod" TYPE="SIMILARITY_METHOD">CosineSimilarity</OPTION>
</SCORING_METHOD>
<SCORING_METHOD NAME="graphBased.LexRank">
	<OPTION NAME="DumpingParameter" TYPE="float">0.15</OPTION>
	<OPTION NAME="GraphThreshold" TYPE="float">0.1</OPTION>
	<OPTION NAME="SimilarityMethod" TYPE="SIMILARITY_METHOD">CosineSimilarity</OPTION>
</SCORING_METHOD>
<SCORING_METHOD NAME="ILP.GenerateModel_ILP">
	<OPTION NAME="SummarySize" TYPE="int">100</OPTION>
	<OPTION NAME="TempName" TYPE="String">entree.ilp</OPTION>
</SCORING_METHOD>

<SUMMARIZE_METHOD NAME="BestIsBetter"></SUMMARIZE_METHOD>
<SUMMARIZE_METHOD NAME="MMR">
	<OPTION NAME="CharLimitBoolean" TYPE="boolean">true</OPTION>
	<OPTION NAME="Size" TYPE="int">100</OPTION>
	<OPTION NAME="Lambda" TYPE="float">0.7</OPTION>
	<OPTION NAME="SimilarityMethod" TYPE="SIMILARITY_METHOD">CosineSimilarity</OPTION>
</SUMMARIZE_METHOD>
<SUMMARIZE_METHOD NAME="ILP"></SUMMARIZE_METHOD>
<SUMMARIZE_METHOD NAME="genetic.GeneticSumm">
	<OPTION NAME="ParentsNumber" TYPE="int">80</OPTION>
	<OPTION NAME="HybridationNumber" TYPE="int">80</OPTION>
	<OPTION NAME="MutationNumber" TYPE="int">160</OPTION>
	<OPTION NAME="RandomNumber" TYPE="int">0</OPTION>
	<OPTION NAME="MaxSummLenght" TYPE="int">100</OPTION>
	<OPTION NAME="GenerationsNb" TYPE="int">2</OPTION>
	<OPTION NAME="MaxMutatedGenes" TYPE="int">1</OPTION>
	<OPTION NAME="GeneticScorerMethod" TYPE="GENETIC_SCORER">JSBigramScorer</OPTION>
	<OPTION NAME="FirstSentenceConceptsFactor" TYPE="float">1.0</OPTION>
	<OPTION NAME="Delta" TYPE="float">0.0005</OPTION>
</SUMMARIZE_METHOD>

<COMPATIBILITY NAME="TF_IDF">
	<SCORING_METHOD NAME=""></SCORING_METHOD>
	<SCORING_METHOD NAME="TF_IDF.Centroid"></SCORING_METHOD>
	<SCORING_METHOD NAME="TF_IDF.ScoringSentenceTF_IDF"></SCORING_METHOD>
	<SCORING_METHOD NAME="graphBased.LexRank"></SCORING_METHOD>
	<SUMMARIZE_METHOD NAME="genetic.GeneticSumm"></SCORING_METHOD>
	<SUMMARIZE_METHOD NAME="BestIsBetter"></SUMMARIZE_METHOD>
	<SUMMARIZE_METHOD NAME="MMR"></SUMMARIZE_METHOD>
</COMPATIBILITY>
<COMPATIBILITY NAME="LDA">
	<SCORING_METHOD NAME="LDA.ScoringLDA"></SCORING_METHOD>
	<SCORING_METHOD NAME="graphBased.LexRank"></SCORING_METHOD>
	<SUMMARIZE_METHOD NAME="BestIsBetter"></SUMMARIZE_METHOD>
	<SUMMARIZE_METHOD NAME="MMR"></SUMMARIZE_METHOD>
</COMPATIBILITY>
<COMPATIBILITY NAME="ILP">
	<SCORING_METHOD NAME="ILP.GenerateModel_ILP"></SCORING_METHOD>
	<SUMMARIZE_METHOD NAME="ILP"></SUMMARIZE_METHOD>
</COMPATIBILITY>

<SIMILARITY_METHOD>
	<NAME>CosineSimilarity</NAME>
	<NAME>JaccardSimilarity</NAME>
	<NAME>JensenShannonSimilarity</NAME>
</SIMILARITY_METHOD>

<GENETIC_SCORER>
	<NAME>CosDivScorer</NAME>
	<NAME>CosineScorer</NAME>
	<NAME>DiversityScorer</NAME>
	<NAME>JSBigramScorer</NAME>
	<NAME>JSBigramUniqueProbScorer</NAME>
	<NAME>JSInterpolation</NAME>
	<NAME>JSUnigramScorer</NAME>
	<NAME>LengthScorer</NAME>
	<NAME>PosScorer</NAME>
	<NAME>SimpleNGramDiversity</NAME>
	<NAME>SimpleNGramScorer</NAME>
</GENETIC_SCORER>
